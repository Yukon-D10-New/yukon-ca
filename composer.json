{
  "name": "evolvingweb/test",
  "description": "Yukon",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.3",
    "acquia/blt": "^13.7",
    "composer/installers": "^1.7",
    "cweagans/composer-patches": "^1.7",
    "drupal/admin_toolbar": "^3.3",
    "drupal/advanced_help": "1.x-dev@dev",
    "drupal/ajax_comments": "1.x-dev@dev",
    "drupal/allowed_formats": "^1.3",
    "drupal/better_exposed_filters": "^6.0",
    "drupal/block_class": "^2.0",
    "drupal/classy": "^1.0",
    "drupal/color_field": "^3.0",
    "drupal/config_split": "^1.4",
    "drupal/core-composer-scaffold": "^10.0",
    "drupal/core-project-message": "^10.0",
    "drupal/core-recommended": "^10.0",
    "drupal/cshs": "^4.0",
    "drupal/date_recur": "^3.2",
    "drupal/devel": "^5.1",
    "drupal/diff": "^1.1",
    "drupal/ds": "^3.15",
    "drupal/environment_indicator": "^4.0",
    "drupal/epp": "^1.3",
    "drupal/extlink": "^1.3",
    "drupal/facets": "^2.0",
    "drupal/feeds": "^3.0@beta",
    "drupal/feeds_tamper": "^2.0@beta",
    "drupal/fences": "^3.0",
    "drupal/field_group": "^3.4",
    "drupal/field_permissions": "^1.1",
    "drupal/focal_point": "^2.0@alpha",
    "drupal/fontawesome": "^2.24",
    "drupal/geofield": "^1.48",
    "drupal/job_scheduler": "^4.0",
    "drupal/leaflet": "^10.0",
    "drupal/libraries": "^4.0",
    "drupal/link_attributes": "^1.11",
    "drupal/linkchecker": "^2.0@alpha",
    "drupal/linkit": "6.0.x-dev@dev",
    "drupal/menu_block": "^1.10",
    "drupal/menu_link_attributes": "^1.3",
    "drupal/metatag": "^1.22",
    "drupal/migrate_plus": "^6.0",
    "drupal/migrate_tools": "^6.0",
    "drupal/page_manager": "^4.0@RC",
    "drupal/pantheon_advanced_page_cache": "^2.1",
    "drupal/paragraphs": "^1.12",
    "drupal/password_policy": "^4.0",
    "drupal/pathauto": "^1.11",
    "drupal/recaptcha": "^3.0",
    "drupal/redirect": "^1.6",
    "drupal/redis": "^1.6",
    "drupal/reroute_email": "^2.2",
    "drupal/responsive_preview": "^2.0",
    "drupal/scheduler": "^2.0@RC",
    "drupal/scheduler_content_moderation_integration": "^2.0@beta",
    "drupal/search404": "^2.1",
    "drupal/search_api_attachments": "^9.0",
    "drupal/search_api_solr": "^4.2",
    "drupal/search_api_spellcheck": "^4.0",
    "drupal/seckit": "^2.0",
    "drupal/security_review": "^2.0",
    "drupal/single_content_sync": "^1.3",
    "drupal/smtp": "^1.2",
    "drupal/stage_file_proxy": "^1.0",
    "drupal/token": "^1.11",
    "drupal/twig_tweak": "^3.2",
    "drupal/ui_patterns": "^1.2",
    "drupal/username_enumeration_prevention": "^1.3",
    "drupal/video_embed_field": "^2.5",
    "drupal/views_ajax_history": "^1.7",
    "drupal/views_bootstrap": "^5.4",
    "drupal/views_menu_children_filter": "^3.0@RC",
    "drupal/views_tree": "^2.0@RC",
    "drupal/viewsreference": "^1.7",
    "drupal/webform": "6.2.x-dev@dev",
    "drupal/workbench": "^1.4",
    "drupal/workbench_access": "^2.0",
    "drupal/workbench_email": "^3.0",
    "drupal/xmlsitemap": "^1.0",
    "evolvingweb/blinker_addons": "^2.4",
    "evolvingweb/blinker_core": "^2.0",
    "evolvingweb/drupal-ci": "^2.0",
    "evolvingweb/glider": "^3.6",
    "oomphinc/composer-installers-extender": "^2.0",
    "pantheon-systems/drupal-integrations": "^10.0",
    "yukon/addtoany": "^1.16",
    "yukon/conditional_fields": "4.x-dev",
    "yukon/geophp": "8.x-dev",
    "yukon/maxlength": "2.x-dev",
    "yukon/module_missing_message_fixer": "2.x-dev",
    "yukon/pathologic": "8.x-dev",
    "yukon/svg_image": "^1.16"
  },
  "extra": {
    "enable-patching": true,
    "patches": {
      "yukon/conditional_fields": {
        "3286636 - Automated Drupal 10 compatibility fixes": "https://git.drupalcode.org/project/conditional_fields/-/merge_requests/19.diff"
      },
      "yukon/geophp": {
        "3287733 - Automated Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2022-07-22/geophp.1.x-dev.rector.patch"
      },
      "yukon/maxlength": {
        "3295052 - Drupal 10 compatibility": "https://git.drupalcode.org/project/maxlength/-/merge_requests/24.diff"
      },
      "yukon/pathologic": {
        "3289033 - Automated Drupal 10 compatibility fixes": "https://www.drupal.org/files/issues/2022-12-02/3289033-drupal-10-compatibility-14.patch"
      },
      "yukon/svg_image": {
        "3299276 - Automated Drupal 10 compatibility fixes\n": "https://www.drupal.org/files/issues/2023-01-13/svg_image-D10-3299276-22.patch"
      }
    },
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "docroot/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "drush/Commands/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "gitignore": false,
      "locations": {
        "web-root": "./docroot"
      },
      "allowed-packages": ["pantheon-systems/drupal-integrations", "evolvingweb/drupal-ci"],
      "file-mapping": {
        "[web-root]/sites/default/default.services.yml": {
          "mode": "replace",
          "path": "docroot/core/assets/scaffold/files/default.services.yml",
          "overwrite": false
        },
        "[web-root]/sites/default/default.settings.php": {
          "mode": "replace",
          "path": "docroot/core/assets/scaffold/files/default.settings.php",
          "overwrite": false
        }
      }
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.ewdev.ca"
    },
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "yukon/addtoany",
        "type": "drupal-module",
        "version": "1.16",
        "source": {
          "url": "https://git.drupalcode.org/project/addtoany.git",
          "type": "git",
          "reference": "8.x-1.x"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "yukon/conditional_fields",
        "type": "drupal-module",
        "version": "4.x-dev",
        "source": {
          "url": "https://git.drupalcode.org/project/conditional_fields",
          "type": "git",
          "reference": "4.x"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "yukon/geophp",
        "type": "drupal-module",
        "version": "8.x-dev",
        "source": {
          "url": "https://git.drupalcode.org/project/geophp",
          "type": "git",
          "reference": "8.x-1.x"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "yukon/maxlength",
        "type": "drupal-module",
        "version": "2.x-dev",
        "source": {
          "url": "https://git.drupalcode.org/project/maxlength",
          "type": "git",
          "reference": "2.0.x"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "yukon/module_missing_message_fixer",
        "type": "drupal-module",
        "version": "2.x-dev",
        "source": {
          "url": "https://git.drupalcode.org/project/module_missing_message_fixer",
          "type": "git",
          "reference": "2.0.x"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "yukon/pathologic",
        "type": "drupal-module",
        "version": "8.x-dev",
        "source": {
          "url": "https://git.drupalcode.org/project/pathologic",
          "type": "git",
          "reference": "8.x-1.x"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "yukon/svg_image",
        "type": "drupal-module",
        "version": "1.16",
        "source": {
          "url": "https://git.drupalcode.org/project/svg_image",
          "type": "git",
          "reference": "8.x-1.x"
        }
      }
    }
  ],
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "acquia/blt": true,
      "acquia/blt-phpcs": true,
      "oomphinc/composer-installers-extender": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "drupal/core-project-message": true
    }
  },
  "autoload": {
    "psr-4": {
      "Evolvingweb\\": "blt/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "blt internal:create-project:init-repo"
    ],
    "nuke": [
      "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
      "@composer clearcache --ansi",
      "@composer install --ansi"
    ]
  },
  "require-dev": {
    "acquia/blt-phpcs": "^1.0",
    "mglaman/phpstan-drupal": "^1.1",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0"
  }
}
